{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@noloback/addresses.controller": [
        "libs/addresses.controller/src/index.ts"
      ],
      "@noloback/addresses.service": ["libs/addresses.service/src/index.ts"],
      "@noloback/auth.controller": ["libs/auth.controller/src/index.ts"],
      "@noloback/auth.service": ["libs/auth.service/src/index.ts"],
      "@noloback/cities.controller": ["libs/cities.controller/src/index.ts"],
      "@noloback/cities.service": ["libs/cities.service/src/index.ts"],
      "@noloback/countries.controller": [
        "libs/countries.controller/src/index.ts"
      ],
      "@noloback/countries.service": ["libs/countries.service/src/index.ts"],
      "@noloback/departments.controller": [
        "libs/departments.controller/src/index.ts"
      ],
      "@noloback/departments.service": [
        "libs/departments.service/src/index.ts"
      ],
      "@noloback/guards": ["models/guards"],
      "@noloback/jwt": ["models/jwt"],
      "@noloback/locations.controller": [
        "libs/locations.controller/src/index.ts"
      ],
      "@noloback/locations.service": ["libs/locations.service/src/index.ts"],
      "@noloback/logger-lib": ["libs/logger/src/index.ts"],
      "@noloback/models": ["models/index.ts"],
      "@noloback/models/swagger": ["models/swagger/index.ts"],
      "@noloback/prisma-client-base": ["libs/prisma/client/base/src/index.ts"],
      "@noloback/prisma-client-logs": ["libs/prisma/client/logs/src/index.ts"],
      "@noloback/prisma-client-translator": [
        "libs/prisma/client/translator/src/index.ts"
      ],
      "@noloback/prisma-lib": ["libs/prisma/src/index.ts"],
      "@noloback/prisma-schema-base": ["libs/prisma/schema/base/src/index.ts"],
      "@noloback/prisma-schema-logs": ["libs/prisma/schema/logs/src/index.ts"],
      "@noloback/prisma-schema-translator": [
        "libs/prisma/schema/translator/src/index.ts"
      ],
      "@noloback/register.controller": [
        "libs/register.controller/src/index.ts"
      ],
      "@noloback/roles": ["models/roles"],
      "@noloback/translator-lib": ["libs/translator/src/index.ts"],
      "@noloback/users.controller": ["libs/users.controller/src/index.ts"],
      "@noloback/users.service": ["libs/users.service/src/index.ts"],
      "@noloback/video.controller": ["libs/video.controller/src/index.ts"],
      "@noloback/video.service": ["libs/video.service/src/index.ts"]
    },
    "types": ["Multer"]
  },
  "exclude": ["node_modules", "tmp"]
}
