{
  "name": "nolovideo",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/nolovideo/src",
  "projectType": "application",
  "targets": {
    "build": {
      "dependsOn": [
        {
          "target": "generate-types"
        }
      ],
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/nolovideo",
        "main": "apps/nolovideo/src/main.ts",
        "tsConfig": "apps/nolovideo/tsconfig.app.json",
        "assets": ["apps/nolovideo/src/assets"],
        "isolatedConfig": true,
        "webpackConfig": "apps/nolovideo/webpack.config.js"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "nolovideo:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "nolovideo:build:development"
        },
        "production": {
          "buildTarget": "nolovideo:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/nolovideo/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/nolovideo/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
